{
    "name": "keboola/object-encryptor",
    "description": "JSON object encryption library",
    "license": "MIT",
    "keywords": ["encryption", "json"],
    "authors": [
        {
            "name": "Keboola",
            "email": "devel@keboola.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Keboola\\Json\\": "src",
            "Keboola\\Json\\Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/keboola/legacy-php-encryption"
        }
    ],
    "require": {
        "keboola-legacy/php-encryption": "^0.0",
        "defuse/php-encryption": "^2.1",
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "codeclimate/php-test-reporter": "^0.4",
        "squizlabs/php_codesniffer": "^3.1"
    }
}
